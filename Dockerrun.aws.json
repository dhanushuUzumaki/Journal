{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "react-client",
      "image": "duzumaki/journal:rc-latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "environment": [
        {
          "name": "REACT_CLIENT_PORT",
          "value": 3000
        },
        {
          "name": "GRAPHQL_ENDPOINT",
          "value": "https://dummyendpoint.com"
        }
      ]
    }
  ]
}
